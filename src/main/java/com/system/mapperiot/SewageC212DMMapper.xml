    </sql>
    <select id="selectSewageC212ByORGId" parameterType="string"  resultMap="BaseResultMap">
        select <include refid="BaseResult_Column_List"/>
        from IOT_SEWAGECON_212 t,iot_deviceinfo u
        where u.di_serialnum=t.device_serialnum
        and (u.DI_DEACTIVE &lt; 1 or u.DI_DEACTIVE is null)
        and u.di_nodeid = #{ORGId}
        order by u.di_serialnum asc
    </select>
    <select id="selectSewageC212ByORGIdAndRoleId" resultMap="BaseResultMap">
        select DISTINCT <include refid="BaseResult_Column_List"/>
        from IOT_SEWAGECON_212 t,iot_deviceinfo u,iot_devicerole a
        where u.di_serialnum=t.device_serialnum
        and (u.DI_DEACTIVE &lt; 1 or u.DI_DEACTIVE is null)
        and u.di_serialnum = a.devrole_devnum
        and u.di_nodeid = #{orgId}
        and a.devrole_roleid in
        <foreach collection="roleIds" item="item" index="index" open="(" separator="," close=")">
            #{item}
        </foreach>
        order by u.di_serialnum asc
    </select>
    <select id="selectSewageC212ByDeviceId" parameterType="string"  resultMap="BaseResultMap">
        select <include refid="BaseResult_Column_List"/>
        from IOT_SEWAGECON_212 t,iot_deviceinfo u
        where u.di_serialnum=t.device_serialnum
        and u.di_serialnum = #{sDeviceId}
    </select>
    <select id="selectSewageC212ByDeviceIdAndDate" resultMap="BaseHisResultMap">
        select
        <include refid="BaseHisResult_Column_List"/>
        from IOT_DEVICEINFO a,IOT_HISSEWAGECON_212 b
        WHERE a.DI_SERIALNUM = b.DEVICE_SERIALNUM
        AND a.DI_SERIALNUM = #{deviceId,jdbcType=VARCHAR}
        AND b.SENDDATE BETWEEN to_date(#{startDate,jdbcType=VARCHAR},'yyyy-mm-dd hh24:mi:ss')
        AND to_date(#{endDate,jdbcType=VARCHAR},'yyyy-mm-dd hh24:mi:ss')
        order by b.SENDDATE ASC
    </select>
    <select id="selectPhoneHisSewageC212ByDateAndId" resultMap="BasePhoneHisResultMap">
        select a.di_name,a.di_serialnum,b.*
        from (select t.*,
        row_number() over(partition by t.device_serialnum,to_char(t.senddate,'yyyy-mm-dd') order by t.senddate desc) rn
        from IOT_HISSEWAGECON_212 t
        where t.senddate between to_date(#{startDate,jdbcType=VARCHAR},'yyyy-mm-dd')
        and to_date(#{endDate,jdbcType=VARCHAR},'yyyy-mm-dd')+1
        and t.DEVICE_SERIALNUM = #{deviceId,jdbcType=VARCHAR}
        ) b,iot_deviceinfo a
        where rn = 1 and a.di_serialnum=b.device_serialnum
    </select>
</mapper>